    "node_modules",
    "bower_components",
    "/test/"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1.2.0",
    "iron-ajax": "PolymerElements/iron-ajax#^1.2.0",
    "paper-card": "PolymerElements/paper-card#^1.1.1",
    "numeral": "^1.5.3",
    "iron-image": "PolymerElements/iron-image#^1.2.3",
    "paper-icon-button": "PolymerElements/paper-icon-button#^1.0.7",
    "iron-flex-layout": "PolymerElements/iron-flex-layout#^1.3.1",
    "paper-styles": "PolymerElements/paper-styles#^1.1.4",
    "iron-icons": "PolymerElements/iron-icons#^1.1.3",
    "paper-behaviors": "PolymerElements/paper-behaviors#^1.0.11",
    "flag-icon-css": "lipis/flag-icon-css#^1.4.0"
